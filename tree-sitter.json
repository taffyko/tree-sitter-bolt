{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "bolt",
      "camelcase": "Bolt",
      "title": "Bolt",
      "scope": "source.bolt",
      "file-types": [
        "bolt"
      ],
      "injection-regex": "^bolt$",
      "class-name": "TreeSitterBolt"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Bolt grammar for tree-sitter",
    "authors": [
      {
        "name": "taffyko"
      }
    ],
    "links": {
      "repository": "https://github.com/taffyko/tree-sitter-bolt"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
