          "value": "not"
        },
        {
          "type": "STRING",
          "value": "is"
        },
        {
          "type": "STRING",
          "value": "match"
        }
      ]
    },
    "type_keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "any"
        },
        {
          "type": "STRING",
          "value": "number"
        },
        {
          "type": "STRING",
          "value": "string"
